# -*-yaml-*- vim:syntax=yaml
provider: Cloudflare
login_token: "{{/* (bitwardenFields "item" "<itemid>").token.value */}}"
domains:
  - domain_name: latipun.dev
    sub_domains: [kumori, raspi]
ip_url: https://api.ip.sb/ip
ipv6_url: https://api-ipv6.ip.sb/ip
ip_type: IPv4
interval: 60
resolver: 1.1.1.1
user_agent:
ip_interface: eth0
socks5_proxy:
use_proxy: false
debug_info: false
notify:
  discord:
    enabled: false
    bot_api_token: "{{/* (bitwardenFields "item" "<itemid>").token.value */}}"
    channel: "{{/* (bitwardenFields "item" "<itemid>").token.value */}}"
    message_template: "Domain **{{`{{ .Domain }}`}}** is updated to **{{`{{ .CurrentIP }}`}}**"
